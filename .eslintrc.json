{
  "extends": ["wesbos/typescript"],
  "rules": {
    "react/prop-types": "off",
    "no-use-before-define": "off",
    "camelcase": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5",
        "printWidth": 120
      }
    ],
    "arrow-body-style": ["error", "always"],
    "no-void": "off",
    "@typescript-eslint/no-use-before-define": "off"
  },
  "parserOptions": {}
}
